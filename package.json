{
  "name": "ssr-react-redux-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "11.14.0",
    "npm": "6.9.0"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "clean": "npm-run-all --parallel clean:*",
    "prod": "npm-run-all --parallel prod:*",
    "dev:build-server": "NODE_ENV='development' webpack --config webpack.server.js --watch",
    "dev:build-client": "NODE_ENV='development' webpack --config webpack.client.js --watch",
    "dev:run-server": "nodemon --watch dist --exec 'node dist/server_bundle.js'",
    "clean:client-build": "rimraf ./public/client.js ",
    "clean:server-build": "rimraf ./dist && mkdir dist",
    "prod:build-server": "NODE_ENV='production' webpack --config webpack.server.js",
    "prod:build-client": "NODE_ENV='production' webpack --config webpack.client.js",
    "run:prod-server": "node dist/server_bundle.js",
    "start": "node dist/server_bundle.js"
  },
  "browserslist": [
    "last 10 version",
    "> 0.2%",
    "maintained node versions",
    "not dead"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
